{
  "name": "My  bc",
  "version": "1.0",
  "manifest_version": 3,

  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting"
    
  ],
  "action": {
    "default_popup": "popup/popup.html"
  
  }
  
}

